{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Mikael Boutin <mikaelboutin.dw@gmail.com>",
  "main": "./dist/index.js",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@types/styled-components": "^4.4.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "formik": "^2.0.4",
    "gatsby": "^2.17.16",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-manifest": "^2.2.28",
    "gatsby-plugin-offline": "^3.0.21",
    "gatsby-plugin-react-helmet": "^3.1.15",
    "gatsby-plugin-sharp": "^2.3.1",
    "gatsby-plugin-typescript": "^2.1.18",
    "gatsby-source-filesystem": "^2.1.37",
    "gatsby-transformer-sharp": "^2.3.5",
    "immer": "^5.0.0",
    "omit.js": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-intl": "^3.6.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-observable": "^1.2.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.3",
    "styled-components": "^4.4.1",
    "typescript": "^3.7.2",
    "yup": "^0.27.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "npm run format:src && npm run format:root",
    "format:src": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:root": "prettier --write \"./*.js\"",
    "transpile": "cross-env NODE_ENV=production npm run build && babel src/_components/ --extensions \".ts,.tsx\" --ignore \"**/__tests__/**\" --ignore \"**/*.d.ts\" -d dist --copy-files && rimraf \"dist/**/__tests__\"",
    "clean": "rm -rf .cache public dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.22",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.12.0",
    "redux-devtools-extension": "^2.13.8",
    "source-map-support": "^0.5.16",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DWboutin/gatsby-react-intl-starter"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run transpile && git add dist/"
    }
  }
}
